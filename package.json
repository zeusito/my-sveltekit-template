		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.8",
		"axios": "^1.9.0",
		"daisyui": "^5.0.43",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.0",
		"globals": "^16.2.0",
		"pino": "^9.7.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.33.10",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0",
		"vite": "^6.3.5",
